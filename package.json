{
  "name": "monaco-auto-typings",
  "version": "1.0.0",
  "description": "provides automatic dependency type completion for Monaco Editor",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "npx deno run -A test/index.ts",
    "preview": "open examples/basic-usage.html"
  },
  "keywords": [
    "monaco-editor",
    "typescript",
    "autocomplete",
    "intellisense",
    "code-editor",
    "types",
    "npm",
    "jsr"
  ],
  "author": {
    "name": "Monaco Auto Typings",
    "email": "contact@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellopao/monaco-auto-typings.git"
  },
  "bugs": {
    "url": "https://github.com/hellopao/monaco-auto-typings/issues"
  },
  "homepage": "https://github.com/hellopao/monaco-auto-typings#readme",
  "dependencies": {
    "@andrewbranch/untar.js": "^1.0.3",
    "@std/async": "npm:@jsr/std__async@^1.0.13",
    "@std/bytes": "npm:@jsr/std__bytes@^1.0.6",
    "@std/collections": "npm:@jsr/std__collections@^1.1.2",
    "@std/path": "npm:@jsr/std__path@^1.1.1",
    "is-builtin-module": "^5.0.0",
    "pako": "^2.1.0",
    "ts-morph": "^26.0.0"
  },
  "peerDependencies": {
    "monaco-editor": "^0.52.2"
  },
  "devDependencies": {
    "vite": "npm:rolldown-vite@latest"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
